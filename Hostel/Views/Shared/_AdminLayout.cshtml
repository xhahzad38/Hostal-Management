<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <script src="~/Scripts/signal.js"></script>
    <script src="~/Scripts/myscript.js"></script>
    <script src="~/signalr/js"></script>
    <link href="~/Content/sb-admin-2.min.css" rel="stylesheet" />
    <link href="~/Content/metisMenu.min.css" rel="stylesheet" />
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-2.2.3.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/metisMenu.min.js"></script>
    <script src="~/Scripts/sb-admin-2.min.js"></script>
    <script src="~/Scripts/jquery-ui.js"></script>
    <link href="~/Content/jquery-ui.css" rel="stylesheet" />
    <link href="~/Content/jquery-ui.structure.css" rel="stylesheet" />
    <link href="~/Content/jquery-ui.theme.css" rel="stylesheet" />
    <script src="~/Scripts/jquery.canvasjs.min.js"></script>



</head>
<body>
    <div id="wrapper">
        <!-- Navigation -->
        <nav  class="navbar navbar-default navbar-static-top" role="navigation" style="margin-bottom: 0">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="~/Admin">@Session["HostelTitle"]</a>
            </div>
            <!-- /.navbar-header -->
            <ul class="nav navbar-top-links navbar-right bg-info">
                <!-- /.dropdown -->

                <li class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                          <span class="glyphicon glyphicon-user"></span>  Admin <span class="glyphicon glyphicon-menu-down"></span>
                    </a>
                    <ul class="dropdown-menu dropdown-user">
                        <li>
                            <a href="~/User/ChangePassword"><span class="glyphicon glyphicon-lock"></span> Change Password</a>
                        </li>
                        <li class="divider"></li>
                        <li>
                            <a style="color:darkred" href="~/User/Logout"><span class="glyphicon glyphicon-log-out"></span> Logout</a>
                        </li>
                    </ul>
                    <!-- /.dropdown-user -->
                </li>
                <!-- /.dropdown -->
            </ul>
            <!-- /.navbar-top-links -->
            <div class="navbar-default sidebar" role="navigation">
                <div class="sidebar-nav navbar-collapse">
                    <ul class="nav" id="side-menu">
                        <li>
                            <h3>&nbsp&nbsp<span class="glyphicon glyphicon-list pull-right"></span>MENU</h3>
                        </li>
                        <li>
                            <a href="~/Admin"> <span class="glyphicon glyphicon-dashboard"></span>  Dashboard</a>
                        </li>


                        <li>
                            <a href="#"><span class="glyphicon glyphicon-cog"></span> Manage Hostel <span class="glyphicon glyphicon-menu-right pull-right"></span></a>
                            <ul class="nav nav-second-level">
                                <li>
                                    <a href="~/Hostel/Building">Buildings <span class="glyphicon glyphicon-menu-right pull-right"></span></a>
                                    <ul class="nav nav-third-level">
                                        <li>
                                            <a href="~/Building/Create">Create New</a>
                                        </li>
                                        <li>
                                            <a href="~/Building/">View Buildings</a>
                                        </li>

                                    </ul>
                                    <!-- /.nav-third-level -->
                                </li>

                                <li>
                                    <a href="~/RoomCategory/">Room Category <span class="glyphicon glyphicon-menu-right pull-right"></span></a>
                                    <ul class="nav nav-third-level">
                                        <li>
                                            <a href="~/RoomCategory/Create">Create New</a>
                                        </li>
                                        <li>
                                            <a href="~/RoomCategory/">View Categories</a>
                                        </li>
                                    </ul>
                                    <!-- /.nav-third-level -->
                                </li>
                                <li>
                                    <a href="~/Room/">Rooms <span class="glyphicon glyphicon-menu-right pull-right"></span></a>
                                    <ul class="nav nav-third-level">
                                        <li>
                                            <a href="~/Room/Create">Create New</a>
                                        </li>
                                        <li>
                                            <a href="~/Room/">View Rooms <span class="glyphicon glyphicon-menu-right pull-right"></span></a>
                                            <ul class="nav nav-third-level">

                                                @{
                                                    foreach (var b in (List<SelectListItem>)Session["BuildingList"])
                                                    {
                                                        <li>
                                                            <a href="~/Room/Index/@b.Value">@b.Text</a>
                                                        </li>
                                                    }
                                                }

                                            </ul>
                                        </li>
                                    </ul>
                                    <!-- /.nav-third-level -->
                                </li>

                                <li>
                                    <a href="#">Mess <span class="glyphicon glyphicon-menu-right pull-right"></span></a>
                                    <ul class="nav nav-third-level">
                                        <li>
                                            <a href="~/Admin/AddMessCharges">Add New Charges</a>
                                        </li>
                                        <li>
                                            <a href="~/Admin/GetMessCharges">View Charges</a>
                                        </li>
                                        <li>
                                            <a href="~/Admin/AddMessDishes">Add New Dishes</a>
                                        </li>
                                        <li>
                                            <a href="~/Admin/MessDishesIndex">View Dishes</a>
                                        </li>
                                    </ul>
                                    <!-- /.nav-third-level -->
                                </li>
                            </ul>
                            <!-- /.nav-second-level -->
                        </li>

                        <li>
                            <a href="#"><span class="glyphicon glyphicon-briefcase"></span> Manage Employee <span class="glyphicon glyphicon-menu-right pull-right"></span></a>
                            <ul class="nav nav-second-level">
                                <li>
                                    <a href="~/Position/">Positions <span class="glyphicon glyphicon-menu-right pull-right"></span></a>
                                    <ul class="nav nav-third-level">
                                        <li>
                                            <a href="~/Position/Create">Create New</a>
                                        </li>
                                        <li>
                                            <a href="~/Position/">View Positions</a>
                                        </li>
                                    </ul>
                                    <!-- /.nav-third-level -->
                                </li>
                                <li>
                                    <a href="~/Employee/">Employees <span class="glyphicon glyphicon-menu-right pull-right"></span></a>
                                    <ul class="nav nav-third-level">
                                        <li>
                                            <a href="~/Employee/Create">Add New</a>
                                        </li>
                                        <li>
                                            <a href="~/Employee/">View Employees<span class="glyphicon glyphicon-menu-right pull-right"></span></a>
                                            <ul class="nav nav-third-level">
                                                <li>
                                                    <a href="~/Employee/">All</a>
                                                </li>
                                                @{
                                                    foreach (var b in (List<SelectListItem>)Session["BuildingList"])
                                                    {
                                                        <li>
                                                            <a href="~/Employee/Index/@b.Value">@b.Text</a>
                                                        </li>
                                                    }
                                                }

                                            </ul>
                                        </li>
                                    </ul>
                                    <!-- /.nav-third-level -->
                                </li>
                            </ul>
                            <!-- /.nav-second-level -->
                        </li>

                        <li>
                            <a href="#"> <span class="glyphicon glyphicon-star-empty"></span>  Hostel Members <span class="glyphicon glyphicon-menu-right pull-right"></span></a>
                            <ul class="nav nav-second-level">

                                @{
                                    foreach (var b in (List<SelectListItem>)Session["BuildingList"])
                                    {
                                        <li>
                                            <a href="~/Member/AdminView/@b.Value">@b.Text</a>
                                        </li>
                                    }
                                }

                            </ul>

                        </li>

                        <li>
                            <a href="#"><span class="glyphicon glyphicon-usd"></span> Accounting <span class="glyphicon glyphicon-menu-right pull-right"></span></a>
                            <ul class="nav nav-second-level">
                                <li>
                                    <a href="~/EmployeeSalary/">Employee Salary <span class="glyphicon glyphicon-menu-right pull-right"></span></a>
                                    <ul class="nav nav-third-level">
                                        <li>
                                            <a href="~/EmployeeSalary/SalaryCreate">Create New</a>
                                        </li>
                                        <li>
                                            <a href="~/EmployeeSalary/">Paid Salaries</a>
                                        </li>
                                    </ul>
                                    <!-- /.nav-third-level -->
                                </li>
                                <li>
                                    <a href="~/Bill/">Building Bills <span class="glyphicon glyphicon-menu-right pull-right"></span></a>
                                    <ul class="nav nav-third-level">
                                        <ul class="nav nav-second-level">

                                            @{
                                                foreach (var b in (List<SelectListItem>)Session["BuildingList"])
                                                {
                                                    <li>
                                                        <a href="~/Bill/Index/@b.Value">@b.Text</a>
                                                    </li>
                                                }
                                            }

                                        </ul>

                                    </ul>
                                    <!-- /.nav-third-level -->
                                </li>
                                <li>
                                    <a href="~/Bill/">Purchases <span class="glyphicon glyphicon-menu-right pull-right"></span></a>
                                    <ul class="nav nav-third-level">
                                        <ul class="nav nav-second-level">

                                            @{
                                                foreach (var b in (List<SelectListItem>)Session["BuildingList"])
                                                {
                                                    <li>
                                                        <a href="~/Purchase/AdminView/@b.Value">@b.Text</a>
                                                    </li>
                                                }
                                            }

                                        </ul>

                                    </ul>
                                    <!-- /.nav-third-level -->
                                </li>
                                <li>
                                    <a href="~/Bill/">Member Payments <span class="glyphicon glyphicon-menu-right pull-right"></span></a>
                                    <ul class="nav nav-third-level">
                                        <ul class="nav nav-second-level">

                                            @{
                                                foreach (var b in (List<SelectListItem>)Session["BuildingList"])
                                                {
                                                    <li>
                                                        <a href="~/MemberPayment/Index/@b.Value">@b.Text</a>
                                                    </li>
                                                }
                                            }

                                        </ul>

                                    </ul>
                                    <!-- /.nav-third-level -->
                                </li>
                            </ul>
                            <!-- /.nav-second-level -->
                        </li>
                        <li>
                            <a href="#"><span class="glyphicon glyphicon-wrench"></span> Visitor logs <span class="glyphicon glyphicon-menu-right pull-right"></span></a>
                            <ul class="nav nav-second-level">
                                <li>
                                    <a href="~/Admin/AddVisitors">Add Visitors</a>
                                </li>
                                <li>
                                    <a href="~/Admin/VisitorIndex">View Visitors</a>
                                </li>
                            </ul>
                            <!-- /.nav-second-level -->
                        </li>

                        <li>
                            <a href="~/Admin/Report"><span class="glyphicon glyphicon-list-alt"></span> Reports </a>

                        </li>

                        <li>
                            <a href="#"><span class="glyphicon glyphicon-wrench"></span> Settings <span class="glyphicon glyphicon-menu-right pull-right"></span></a>
                            <ul class="nav nav-second-level">
                                <li>
                                    <a href="~/Hostel/">General Settings</a>
                                </li>

                            </ul>
                            <!-- /.nav-second-level -->
                        </li>


                    </ul>
                </div>
                <!-- /.sidebar-collapse -->
            </div>
            <!-- /.navbar-static-side -->
        </nav>
        <!-- Page Content -->
        <div id="page-wrapper">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12">
                        <br />
                        @if (TempData["Success"] != null)
                        {
                            <div class="alert alert-success alert-dismissable fade in">
                                <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                                <strong>Success!</strong> @TempData["Success"]
                            </div>
                        }
                        @if (TempData["Info"] != null)
                        {
                            <div class="alert alert-info alert-dismissable fade in">
                                <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                                <strong>Information!</strong> @TempData["Info"]
                            </div>
                        }
                        @if (TempData["Failed"] != null)
                        {
                            <div class="alert alert-danger alert-dismissable fade in">
                                <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                                <strong>Failed!</strong> @TempData["Failed"]
                            </div>
                        }
                        @RenderBody()
                    </div>
                    <!-- /.col-lg-12 -->
                </div>
                <!-- /.row -->
            </div>
            <!-- /.container-fluid -->
        </div>
        <!-- /#page-wrapper -->
    </div>
    <!-- /#wrapper -->
    <!-- jQuery -->
    
</body>
</html>

