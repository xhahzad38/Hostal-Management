@model IEnumerable<VisitorLogs>

@{
    ViewBag.Title = "VisitorIndex";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>VisitorIndex</h2>

<p>
    @Html.ActionLink("Create New", "AddVisitors")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.VisitorName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VisitorCNIC)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VisitorCardNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StudentName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateAdded)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.VisitorName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VisitorCNIC)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VisitorCardNo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StudentName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateAdded)
        </td>
        @*<td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>*@
    </tr>
}

</table>
